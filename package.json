{
  "name": "angular-webpack-workflow",
  "version": "1.1.0",
  "description": "A workflow for Angular made with Webpack",
  "scripts": {
    "build": "npx webpack --bail --progress --profile",
    "server": "npx webpack-dev-server --history-api-fallback --inline --progress",
    "test": "npx karma start",
    "test-watch": "npx karma start --auto-watch --no-single-run",
    "start": "npm run server"
